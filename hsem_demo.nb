Cell[BoxData[
 RowBox[{
  RowBox[{"SphericalExpansionDiffOp", "[", "k_", "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Mod", "[", 
      RowBox[{"k", ",", "2"}], "]"}], "\[Equal]", "0"}], ",", 
    RowBox[{
     RowBox[{
      RowBox[{"1", "/", 
       RowBox[{"(", 
        RowBox[{"2", "Pi"}], ")"}]}], 
      RowBox[{"Integrate", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Cos", "[", "\[Phi]", "]"}], "^", 
         RowBox[{"(", "k", ")"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Phi]", ",", "0", ",", 
          RowBox[{"2", "Pi"}]}], "}"}]}], "]"}], 
      RowBox[{"AngLatSum", "[", 
       RowBox[{
        RowBox[{"\[Nu]", "-", "k"}], ",", "0"}], "]"}], 
      RowBox[{"PolyLaplaceDiffOp", "[", "k", "]"}]}], "+", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"1", "/", "Pi"}], "*", 
        RowBox[{"Integrate", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"Cos", "[", "\[Phi]", "]"}], "^", 
            RowBox[{"(", "k", ")"}]}], "*", 
           RowBox[{"Cos", "[", 
            RowBox[{"2", "n", "*", "\[Phi]"}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"\[Phi]", ",", "0", ",", 
            RowBox[{"2", "Pi"}]}], "}"}]}], "]"}], "*", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"AngLatSum", "[", 
           RowBox[{
            RowBox[{"\[Nu]", "-", "k", "+", 
             RowBox[{"2", "n"}]}], ",", 
            RowBox[{"2", "n"}]}], "]"}], "*", 
          RowBox[{"LaplaceSolidDiffOp", "[", 
           RowBox[{
            RowBox[{"k", "-", 
             RowBox[{"2", "n"}]}], ",", 
            RowBox[{"2", "n"}]}], "]"}]}], ")"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"n", ",", "1", ",", 
         RowBox[{"k", "/", "2"}]}], "}"}]}], "]"}]}], ",", "0"}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.810622179817575*^9, 3.810622219550869*^9}, 
   3.810622271644946*^9, {3.810622317755239*^9, 3.810622326405851*^9}, {

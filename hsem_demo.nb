Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"GenerateSolidHarmonic2D", "[", "k_", "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"Cos", "[", 
        RowBox[{"k", "*", "\[Phi]"}], "]"}], "*", 
       RowBox[{
        RowBox[{"Sqrt", "[", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"x1", "^", "2"}], "+", 
           RowBox[{"x2", "^", "2"}]}], ")"}], "]"}], "^", "k"}]}], "/.", 
      RowBox[{"{", 
       RowBox[{"\[Phi]", "\[Rule]", 
        RowBox[{"ArcTan", "[", 
         RowBox[{"x1", "/", "x2"}], "]"}]}], "}"}]}], "//", "TrigExpand"}], "//",
     "FullSimplify"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.810619840673874*^9, 3.810619848857357*^9}, {
  3.810619893152912*^9, 3.8106198936102333`*^9}},
 CellLabel->"In[32]:=",ExpressionUUID->"b6ce7d96-071e-4c69-b6f3-64c8cf238931"],

Cell["Generate a differential operator out of the solid harmonics.", "Text",
 CellChangeTimes->{{3.810871662874597*^9, 3.8108717051752357`*^9}, 
   3.821168876999366*^9},ExpressionUUID->"9dc9e9d2-0e58-4633-a174-\
92999d1ed26e"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"SolidDiffOp", "[", "k_", "]"}], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"GenerateSolidHarmonic2D", "[", "k", "]"}], "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"x1", "^", "n1_"}], " ", 
         RowBox[{"x2", "^", "_n2"}]}], " ", "\[Rule]", 
        RowBox[{"DiffOp", "[", 
         RowBox[{"n1", ",", "n2"}], "]"}]}], "}"}]}], ")"}], "/.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"x1", "^", "n1_"}], "\[Rule]", 
       RowBox[{"DiffOp", "[", 
        RowBox[{"n1", ",", "0"}], "]"}]}], ",", 
      RowBox[{
       RowBox[{"x2", "^", "n2_"}], "\[Rule]", 
       RowBox[{"DiffOp", "[", 
        RowBox[{"0", ",", "n2"}], "]"}]}]}], "}"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.810619987387576*^9, 3.810620102252507*^9}},
 CellLabel->"In[34]:=",ExpressionUUID->"a7751b73-14ca-497d-9abb-c75d4a43aca5"],


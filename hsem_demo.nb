Cell[BoxData[
 RowBox[{
  RowBox[{"Lattice", " ", 
   RowBox[{"Sum", ":", 
    FractionBox[
     SuperscriptBox["x1", 
      RowBox[{"2", "n"}]], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["x1", "2"], "+", 
        SuperscriptBox["x2", "2"]}], ")"}], 
      RowBox[{"p", "+", "n"}]]]}]}], ",", " ", 
  RowBox[{
   RowBox[{"double", " ", "sum", " ", "offers", " ", "exponential", " ", 
    RowBox[{"convergence", ".", " ", "Only"}], " ", "works", " ", "for", " ", 
    "n"}], ">", 
   RowBox[{"0", "!"}]}]}]], "Text",
 CellChangeTimes->{{3.8108713196293573`*^9, 
  3.810871360433776*^9}},ExpressionUUID->"d996133e-2ea5-4712-b74e-\
453724add683"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"LatticeSumPart", "[", 
    RowBox[{"n_", ",", "s_"}], "]"}], "=", 
   RowBox[{
    RowBox[{"2", "*", 
     RowBox[{"Sqrt", "[", "Pi", "]"}], "*", 
     RowBox[{"Gamma", "[", 
      RowBox[{"s", "+", "n", "-", 
       RowBox[{"1", "/", "2"}]}], "]"}], "*", 
     RowBox[{
      RowBox[{"Zeta", "[", 
       RowBox[{
        RowBox[{"2", "s"}], "-", "1"}], "]"}], "/", 
      RowBox[{"Gamma", "[", 
       RowBox[{"s", "+", "n"}], "]"}]}]}], "+", 
    RowBox[{"8", "*", 
     RowBox[{
      RowBox[{"Pi", "^", "s"}], "/", 
      RowBox[{"Gamma", "[", 
       RowBox[{"s", "+", "n"}], "]"}]}], 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"p2", "/", "p1"}], ")"}], "^", 
         RowBox[{"(", 
          RowBox[{"s", "-", 
           RowBox[{"1", "/", "2"}]}], ")"}]}], "*", 
        RowBox[{"p1", "^", "n"}], "*", 
        RowBox[{"p2", "^", "n"}], "*", 
        RowBox[{"Pi", "^", "n"}], "*", 
        RowBox[{"BesselK", "[", 
         RowBox[{
          RowBox[{"s", "+", "n", "-", 
           RowBox[{"1", "/", "2"}]}], ",", 
          RowBox[{"2", "Pi", "*", "p1", "*", "p2"}]}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"p1", ",", "1", ",", "pUpper"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"p2", ",", "1", ",", "pUpper"}], "}"}]}], "]"}]}]}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.810444251498548*^9, 3.8104442795115128`*^9}, {
  3.8104443191820593`*^9, 3.810444437391189*^9}, {3.810444578972144*^9, 
  3.810444580447839*^9}},
 CellLabel->"In[27]:=",ExpressionUUID->"546d7b6f-220f-4bf6-97b4-4e58447cfcfa"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Lattice", " ", 
    RowBox[{"Sum", ":", 
     FractionBox[
      RowBox[{"SolidHarmonic", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"x1", ",", "x2"}], "}"}], ",", "k"}], "]"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["x1", "2"], "+", 
         SuperscriptBox["x2", "2"]}], ")"}], 
       RowBox[{"\[Nu]", "/", "2"}]]]}]}], "=", 
   FractionBox[
    RowBox[{
     RowBox[{
      RowBox[{"x1", "^", "k"}], "+"}], "..."}], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["x1", "2"], "+", 
       SuperscriptBox["x2", "2"]}], ")"}], 
     RowBox[{"\[Nu]", "/", "2"}]]]}], ",", " ", 
  RowBox[{
  "only", " ", "nonzero", " ", "for", " ", "k", " ", "a", " ", "multiple", 
   " ", "of", " ", "4", " ", "due", " ", "to", " ", "symmetries", " ", "of", 
   " ", "the", " ", "square", " ", 
   RowBox[{"lattice", "."}]}]}]], "Text",
 CellChangeTimes->{{3.8108714564563503`*^9, 
  3.810871531979385*^9}},ExpressionUUID->"6c58a45c-4e8f-4e0c-9329-\
ec70ba036d67"],

Cell[BoxData[
 RowBox[{
  RowBox[{"GenerateLatticeSumAng", "[", "l_", "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Mod", "[", 
      RowBox[{"l", ",", "4"}], "]"}], "\[Equal]", "0"}], ",", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"ChebyshevT", "[", 
            RowBox[{"l", ",", "x"}], "]"}], "-", "1"}], ")"}], "/.", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"x", "^", "n_"}], "->", 
           RowBox[{"LatticeSumPart", "[", 
            RowBox[{
             RowBox[{"n", "/", "2"}], ",", "s"}], "]"}]}], "}"}]}], ")"}], 
       "+", 
       RowBox[{"2", 
        RowBox[{"LatticeSumPart", "[", 
         RowBox[{"1", ",", "s"}], "]"}]}]}], ")"}], "/.", 
     RowBox[{"{", 
      RowBox[{"s", "->", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"\[Nu]", "-", "l"}], ")"}], "/", "2"}]}], "}"}]}], ",", 
    "0"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.8104490746879787`*^9, 3.810449264067762*^9}, 
   3.810449319039547*^9, {3.8106211871693707`*^9, 3.810621264572311*^9}},
 CellLabel->"In[29]:=",ExpressionUUID->"a5726d2d-1868-40c5-af48-e99e9e9ae464"],


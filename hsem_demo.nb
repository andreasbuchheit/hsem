   3.821168866234267*^9, {3.8229757366767683`*^9, 
   3.822975737883801*^9}},ExpressionUUID->"870dc3ee-4145-4ff1-8e7f-\
778f3cd5e452"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"PolyLaplaceDiffOp", "[", "k_", "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"k", "\[Equal]", "0"}], ",", 
     RowBox[{"DiffOp", "[", 
      RowBox[{"0", ",", "0"}], "]"}], ",", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Mod", "[", 
            RowBox[{"k", ",", "2"}], "]"}], "\[Equal]", "0"}], ",", 
          RowBox[{"(", 
           RowBox[{"Expand", "[", 
            RowBox[{
             RowBox[{"Sqrt", "[", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"x1", "^", "2"}], "+", 
                RowBox[{"x2", "^", "2"}]}], ")"}], "]"}], "^", "k"}], "]"}], 
           ")"}], ",", "0"}], "]"}], "/.", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"x1", "^", "n1_"}], " ", 
           RowBox[{"x2", "^", "n2_"}]}], " ", "\[Rule]", 
          RowBox[{"DiffOp", "[", 
           RowBox[{"n1", ",", "n2"}], "]"}]}], "}"}]}], ")"}], "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"x1", "^", "n1_"}], "\[Rule]", 
         RowBox[{"DiffOp", "[", 
          RowBox[{"n1", ",", "0"}], "]"}]}], ",", 
        RowBox[{
         RowBox[{"x2", "^", "n2_"}], "\[Rule]", 
         RowBox[{"DiffOp", "[", 
          RowBox[{"0", ",", "n2"}], "]"}]}]}], "}"}]}], ",", "0"}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.8106226142451363`*^9, 3.810622621740122*^9}, {
  3.8106226606680803`*^9, 3.810622708196068*^9}, {3.810622744080345*^9, 
  3.810622787549642*^9}, {3.81062296101136*^9, 3.810622981717395*^9}, {
  3.8106254873839808`*^9, 3.810625489002372*^9}},
 CellLabel->"In[35]:=",ExpressionUUID->"422d7ee5-8eb2-4181-a8c7-a1266418e799"],

Cell["\<\
Differential Operator, generated from products of poly-Laplacians and solid \
harmonic differential operators.\
\>", "Text",
 CellChangeTimes->{{3.810871742072154*^9, 3.810871765687908*^9}, {
  3.822975740371256*^9, 
  3.822975741521624*^9}},ExpressionUUID->"e2735ef6-927d-4854-8725-\
f47fcb3b9686"],

Cell[BoxData[
 RowBox[{
  RowBox[{"LaplaceSolidDiffOp", "[", 
   RowBox[{"kRadial_", ",", "kAngular_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Mod", "[", 
            RowBox[{"kRadial", ",", "2"}], "]"}], "\[Equal]", "0"}], "&&", 
          RowBox[{
           RowBox[{"Mod", "[", 
            RowBox[{"kAngular", ",", "2"}], "]"}], "\[Equal]", "0"}]}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"x1", "^", "2"}], "+", 
             RowBox[{"x2", "^", "2"}]}], ")"}], "^", 
           RowBox[{"(", 
            RowBox[{"kRadial", "/", "2"}], ")"}]}], "*", 
          RowBox[{"GenerateSolidHarmonic2D", "[", "kAngular", "]"}]}], ",", 
         "0"}], "]"}], "//", "Expand"}], ")"}], "/.", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"x1", "^", "n1_"}], " ", 
        RowBox[{"x2", "^", "n2_"}]}], " ", "\[Rule]", 
       RowBox[{"DiffOp", "[", 
        RowBox[{"n1", ",", "n2"}], "]"}]}], "}"}]}], ")"}], "/.", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{

 RowBox[{
  RowBox[{
   RowBox[{"DiffOp", "[", "l_", "]"}], ":=", 
   RowBox[{"Collect", "[", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Mod", "[", 
         RowBox[{"l", ",", "2"}], "]"}], "\[Equal]", "0"}], ",", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"1", "/", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"2", "k"}], ")"}], "!"}]}], "*", 
           RowBox[{"SphericalExpansionDiffOp", "[", 
            RowBox[{"2", "k"}], "]"}]}], "//", "Expand"}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "0", ",", 
           RowBox[{"l", "/", "2"}]}], "}"}]}], "]"}], ",", "0"}], "]"}], ",", 
     
     RowBox[{"AngLatSum", "[", 
      RowBox[{"_", ",", "_"}], "]"}], ",", "Together"}], "]"}]}], 
  ";"}]], "Input",
